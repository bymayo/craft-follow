{
    "name": "bymayo/follow",
    "description": "Let users follow other users and elements (Categories, Tags etc) to generate lists & feeds.",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "follow"
    ],
    "support": {
        "docs": "https://plugins.bymayo.co.uk/follow/",
        "issues": "https://github.com/bymayo/craft-follow/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "ByMayo",
            "homepage": "https://bymayo.co.uk"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "bymayo\\follow\\": "src/"
        }
    },
    "extra": {
        "name": "Follow",
        "handle": "follow",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/bymayo/craft-follow/craft-4/CHANGELOG.md",
        "components": {
            "followService": "bymayo\\follow\\services\\FollowService"
        },
        "class": "bymayo\\follow\\Follow"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/rector": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
